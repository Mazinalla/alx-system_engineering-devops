#!/usr/bin/env bash
# Using a for loop to display the content of the current directory
for file in *; do
    # Check if the file is not a hidden file (does not start with a dot)
    if [[ ! $file == .* ]]; then
        # Extract the part of the name after the first dash
        name_part=$(echo "$file" | cut -d'-' -f2-)
        # Display the result in a list format
        echo "- $name_part"
    fi
done
